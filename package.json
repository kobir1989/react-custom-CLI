    "fs-extra": "^11.3.0",
    "inquirer": "^8.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kobir1989/react-custom-CLI"
  },
  "devDependencies": {
    "esbuild": "^0.19.12"
  }
}
